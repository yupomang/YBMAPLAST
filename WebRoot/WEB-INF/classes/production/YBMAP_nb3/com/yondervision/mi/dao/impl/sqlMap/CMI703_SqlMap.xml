<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE sqlMap PUBLIC "-//ibatis.apache.org//DTD SQL Map 2.0//EN" "http://ibatis.apache.org/dtd/sql-map-2.dtd" >
<sqlMap namespace="CMI703" >
  <update id="updatePraisecountsByPrimaryKey" parameterClass="com.yondervision.mi.dto.Mi703" >
    update MI703
    set
		PRAISECOUNTS = PRAISECOUNTS+1
    where SEQNO = #seqno:INTEGER#
  </update>
  
	<update id="updatePraisecountsSubByPrimaryKey" parameterClass="com.yondervision.mi.dto.Mi703" >
    update MI703
    set
		PRAISECOUNTS = PRAISECOUNTS-1
    where SEQNO = #seqno:INTEGER#
  </update>
</sqlMap>