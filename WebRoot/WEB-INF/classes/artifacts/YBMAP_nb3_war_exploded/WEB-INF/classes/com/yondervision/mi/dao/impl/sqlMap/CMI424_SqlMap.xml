<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE sqlMap PUBLIC "-//ibatis.apache.org//DTD SQL Map 2.0//EN" "http://ibatis.apache.org/dtd/sql-map-2.dtd">
<sqlMap namespace="CMI424">
	<resultMap class="java.util.HashMap" id="webapi42401">
		<result column="cnt" jdbcType="VARCHAR" property="cnt" />
		<result column="succ" jdbcType="VARCHAR" property="succ" />
		<result column="flag" jdbcType="VARCHAR" property="flag" />
		<result column="fail" jdbcType="VARCHAR" property="fail" />
	</resultMap>
	<resultMap class="java.util.HashMap" id="webapi42402">
		<result column="itemid" jdbcType="VARCHAR" property="itemid" />
		<result column="itemval" jdbcType="VARCHAR" property="itemval" />
		<result column="cnt" jdbcType="VARCHAR" property="cnt" />
		<result column="succ" jdbcType="VARCHAR" property="succ" />
		<result column="fail" jdbcType="VARCHAR" property="fail" />
	</resultMap>
	<select id="webapi42401" parameterClass="java.util.HashMap" resultMap="webapi42401">
	  	select sum(TRANSCNT) cnt, sum(SUCCCNT) succ, sum(FAILCNT) fail, pus_message_type flag 
	  	from mi424 
		where validflag=1 and pus_message_type!='03' 
		<isNotEmpty prepend="and" property="centerid">
			CENTERID= #centerid:VARCHAR#
		</isNotEmpty>
		<isNotEmpty prepend="and" property="pid">
			pid =#pid:VARCHAR#
		</isNotEmpty>
		<isNotEmpty prepend="and" property="startDate">
			transdate &gt;=#startDate:VARCHAR#
		</isNotEmpty>
		<isNotEmpty prepend="and" property="endDate">
			transdate &lt;=#endDate:VARCHAR#
		</isNotEmpty>
		group by pus_message_type
  	</select>
  	<select id="webapi42402" parameterClass="java.util.HashMap" resultMap="webapi42402">
	  	select t2.itemid, t2.itemval, t1.cnt, t1.succ, t1.fail 
	  	from
			(select sum(TRANSCNT) cnt, sum(SUCCCNT) succ, sum(FAILCNT) fail, theme
			from mi424 
			where pus_message_type='03' and validflag=1 
			<isNotEmpty prepend="and" property="centerid">
				CENTERID= #centerid:VARCHAR#
			</isNotEmpty>
			<isNotEmpty prepend="and" property="pid">
				pid =#pid:VARCHAR#
			</isNotEmpty>
			<isNotEmpty prepend="and" property="startDate">
				transdate &gt;=#startDate:VARCHAR#
			</isNotEmpty>
			<isNotEmpty prepend="and" property="endDate">
				transdate &lt;=#endDate:VARCHAR#
			</isNotEmpty>
			group by theme) t1 
			join mi007 t2 on t1.theme = t2.itemid and t2.updicid in (
                     select dicid from mi007 where itemid='message_topic_type') 
  	</select>
</sqlMap>